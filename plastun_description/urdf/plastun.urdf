  <gazebo reference="hokuyo">
    <sensor type="gpu_ray" name="hokuyo_sensor">
      <pose>0 0 0.1 0 0 3.1416</pose>
      <visualize>true</visualize>
      <update_rate>40</update_rate>
      <ray>
        <scan>
          <horizontal>
            <samples>720</samples>
            <resolution>1</resolution>
            <min_angle>-1.470796</min_angle>
            <max_angle>1.470796</max_angle>
          </horizontal>
        </scan>
        <range>
          <min>0.10</min>
          <max>15.0</max>
          <resolution>0.01</resolution>
        </range>
        <noise>
          <type>gaussian</type>
          <!-- Noise parameters based on published spec for Hokuyo laser
               achieving "+-30mm" accuracy at range < 10m.  A mean of 0.0m and
               stddev of 0.01m will put 99.7% of samples within 0.03m of the true
               reading. -->
               <mean>0.0</mean>
               <stddev>0.01</stddev>
             </noise>
           </ray>
           <plugin name="gazebo_ros_hokuyo_sensor" filename="libgazebo_ros_gpu_laser.so">
            <robotNamespace>plastun</robotNamespace>
            <topicName>laser/scan</topicName>
            <frameName>hokuyo_link</frameName>
          </plugin>
        </sensor>
      </gazebo>

      <gazebo reference="lazer_ukazka">
        <sensor type="gpu_ray" name="hokuyo_laser_ukaz">
          <pose>-0.1 0 0.05 0 0 3.1416</pose>
          <visualize>true</visualize>
          <update_rate>40</update_rate>
          <ray>
            <scan>
              <horizontal>
                <samples>6</samples>
                <resolution>1</resolution>
                <min_angle>-0.005</min_angle>
                <max_angle>0.005</max_angle>
              </horizontal>
            </scan>
            <range>
              <min>0.10</min>
              <max>10.0</max>
              <resolution>0.01</resolution>
            </range>
            <noise>
              <type>gaussian</type>
          <!-- Noise parameters based on published spec for Hokuyo laser
               achieving "+-30mm" accuracy at range < 10m.  A mean of 0.0m and
               stddev of 0.01m will put 99.7% of samples within 0.03m of the true
               reading. -->
               <mean>0.0</mean>
               <stddev>0.01</stddev>
             </noise>
           </ray>
           <plugin name="gazebo_ros_hokuyo_laser_ukaz" filename="libgazebo_ros_gpu_laser.so">
            <robotNamespace>plastun</robotNamespace>
            <topicName>laser/scan_ukaz</topicName>
            <frameName>ukaz_link</frameName>
          </plugin>
        </sensor>
      </gazebo>

      <gazebo>
        <plugin name="skid_steer_drive_controller" filename="libgazebo_ros_skid_steer_drive.so">
          <updateRate>100.0</updateRate>
          <robotNamespace>plastun</robotNamespace>
          <leftFrontJoint>wheel_lev_1</leftFrontJoint>
          <rightFrontJoint>wheel_prav_1</rightFrontJoint>
          <leftRearJoint>wheel_lev_3</leftRearJoint>
          <rightRearJoint>wheel_prav_3</rightRearJoint>
          <wheelSeparation>0.4</wheelSeparation>
          <wheelDiameter>0.192</wheelDiameter>
          <robotBaseFrame>chassis</robotBaseFrame>
          <torque>20</torque>
          <commandTopic>cmd_vel</commandTopic>
          <odometryTopic>odom</odometryTopic>
          <odometryFrame>odom</odometryFrame>
          <broadcastTF>true</broadcastTF>
          <covariance_x>0.000100</covariance_x>
          <covariance_y>0.000100</covariance_y>
          <covariance_yaw>0.000100</covariance_yaw>
        </plugin>
      </gazebo>


