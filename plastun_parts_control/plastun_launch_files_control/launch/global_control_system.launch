<launch>

<!-- activate amcl + move base + others -->
  <include file="$(find plastun_navigation)/launch/amcl.launch"/>

<!-- activate move global_control_system (clients for all servers) -->
  <node name="global_control_system" pkg="plastun_global_control_system" type="global_control_system" respawn="false" output="screen"></node>

 <!-- activate image detect server -->
  <node name="image_detect" pkg="plastun_image_detect" type="image_detect" respawn="false" output="screen">
  	<param name="cascad" value="$(find plastun_image_detect)/cascads/plafon_gazebo/cascade.xml" />
	<param name="suscribe_camera_topic" value="/plastun/camera_1/image_raw_1" /> 
 </node>

<!-- activate rotate turret server-->
  <node name="rotate_turret" pkg="plastun_rotate_turret" type="rotate_turret" respawn="false" output="screen"/>

<!-- activate activating laser server -->
<!--  <node name="activate_laser" pkg="plastun_activate_laser" type="activate_laser" respawn="false" output="screen"></node> -->


</launch>
